<div class="container">

  <h1>Latest Notes</h1><br>
  <div class="row">
    <% @notes.each do |note| %>
    <div class="col-md-4">
      <div class="panel panel-default note-wrapper">
        <div class="panel-body">
          <h4><%= link_to note.title, note, class: "white" %></h4>
          <p><%= note.boolean_to_text %></p>
        </div>
      </div>
    </div>
    <% end %>
  </div>
</div>

<script>

  $('document').ready(function(){
    setDiVHeight();
  });

  function setDiVHeight(){
    $('.thumbnail').matchHeight();
    $('.jumbotron').matchHeight();
  }
</script>